PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX catastodim: <https://iisg.amsterdam/catasto/dimension/>
PREFIX catasto: <https://iisg.amsterdam/catasto/>
PREFIX relationcode: <https://iisg.amsterdam/catasto/code/relationToHead/>

SELECT ?agefl (count(?memberflorence) as ?n)

WHERE {
?florence  	catastodim:NUMSER <https://iisg.amsterdam/catasto/code/NUMSER/1> .
?florence		catastodim:householdMember ?memberflorence .
?memberflorence catastodim:age ?agefl .

FILTER(?agefl > 20)

  }
ORDER BY ASC (?agefl)
